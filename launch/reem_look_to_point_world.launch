<launch>
  <arg name="gzworld" default="empty.world"/>
  <param name="/use_sim_time" type="bool" value="true"/>

  <!-- start gazebo -->
  <node name="gazebo" pkg="reem_gazebo" type="run_gazebo" args="$(find reem_tutorials)/worlds/world_wall_REEM_photo.world" output="screen" />

  <!-- start REEM robot -->
  <include file="$(find reem_gazebo)/launch/reem.launch"/>
</launch>


