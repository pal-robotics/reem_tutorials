<robot name="wallWithREEMphoto">
	<link name="wallWithREEMphoto">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />			
			<mass value="4.0" />
			<!-- center of mass (com) is defined w.r.t. link local coordinate system -->
			<origin xyz="0 0 0" />
			<inertia  ixx="0.0" ixy="0.0"  ixz="0.0"  iyy="0.0"  iyz="0.0"  izz="0.0" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="0.05 1.0 2.0" /> <!-- Wall dimensions: thickness width length -->
			</geometry>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="0.05 1.0 2.0" /> <!-- Wall dimensions: thickness width length -->
			</geometry>
		</collision>
	</link>
	<gazebo reference="wallWithREEMphoto">
		<material>Custom/REEM_photo0</material>
	</gazebo>
</robot>
